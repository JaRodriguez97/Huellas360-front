{
  "name": "huellas360",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve -o",
    "c": "ng g c components/infoUser --skip-import",
    "m": "ng g m modules/MapaHuellas/mapaHuellas -m=app --route MapaDeHuellas --flat",
    "s": "ng g s services/Map/mapService",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run Huellas360:serve-ssr",
    "serve:ssr": "node dist/Huellas360/server/main.js",
    "build:ssr": "ng build && ng run Huellas360:server",
    "prerender": "ng run Huellas360:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/platform-server": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@asymmetrik/ngx-leaflet": "^14.0.1",
    "@nguniversal/express-engine": "^14.2.3",
    "express": "^4.15.2",
    "flowbite": "^2.5.1",
    "leaflet": "^1.9.4",
    "postcss": "^8.4.47",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
