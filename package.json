    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/leaflet": "^1.9.14",
    "@types/node": "^14.15.0",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "tailwindcss": "^3.4.15",
    "typescript": "~4.7.2"
